container_commands:
  01_migrate:
    command: "python untitled2/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python untitled2/manage.py collectstatic --noinput"
  03_createsu:
    command: "python untitled2/manage.py createsu"
    leader_only: true
option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "untitled2.settings"
    "PYTHONPATH": "/opt/python/current/app/untitled2:$PYTHONPATH"
    "ALLOWED_HOSTS": ".elasticbeanstalk.com"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "untitled/untitled2/wsgi.py"
  "aws:elasticbeanstalk:container:python:staticfiles":
    /static/: "static/"
